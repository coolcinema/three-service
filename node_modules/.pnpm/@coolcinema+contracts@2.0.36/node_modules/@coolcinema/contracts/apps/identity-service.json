{
  "fullnameOverride": "identity-service",
  "image": {
    "repository": "ghcr.io/coolcinema/identity-service"
  },
  "service": {
    "ports": [
      {
        "name": "grpc",
        "port": 5000,
        "protocol": "TCP"
      },
      {
        "name": "http-main",
        "port": 3000,
        "protocol": "TCP"
      }
    ]
  },
  "ingress": [
    {
      "name": "main",
      "port": 3000,
      "host": "identity-service.coolcinema.local",
      "path": "/"
    }
  ],
  "env": {}
}


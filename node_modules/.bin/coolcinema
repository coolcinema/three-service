#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/dist/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/dist/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/@coolcinema+cli@4.0.34/node_modules:/home/mr/Hellkitchen/education/pet/coolcinema/project/three-service/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/@coolcinema+cli@4.0.34/node_modules/@coolcinema/cli/dist/index.js" "$@"
fi
